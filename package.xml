<?xml version="1.0"?>
<package>
  <name>rosR</name>
  <version>0.0.3</version>
  <description>The rosR package</description>

  <maintainer email="dietrich@ivs.cs.uni-magdeburg.de">Andre Dietrich</maintainer>

  <license>BSD</license>

  <url type="website">http://wiki.ros.org/rosR</url>

  <!-- The *_depend tags are used to specify dependencies -->
  <!-- Dependencies can be catkin packages or system dependencies -->
  <!-- Examples: -->
  <!-- Use build_depend for packages you need at compile time: -->
  <!--   <build_depend>message_generation</build_depend> -->
  <!-- Use buildtool_depend for build tool packages: -->
  <!--   <buildtool_depend>catkin</buildtool_depend> -->
  <!-- Use run_depend for packages you need at runtime: -->
  <!--   <run_depend>message_runtime</run_depend> -->
  <!-- Use test_depend for packages you need only for testing: -->
  <!--   <test_depend>gtest</test_depend> -->

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>rosbag</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>topic_tools</build_depend>
  <build_depend>std_srvs</build_depend>


  <build_depend>swig3.0</build_depend>
  <build_depend>r-cran-rcpp</build_depend>

  <run_depend>rosbag</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>topic_tools</run_depend>
  <run_depend>r-cran-rcpp</run_depend>

  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <!-- You can specify that this package is a metapackage here: -->
    <!-- <metapackage/> -->

    <!-- Other tools can request additional information be placed here -->

  </export>
</package>
